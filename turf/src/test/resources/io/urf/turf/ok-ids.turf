===>urf:
  space-count = <https://example.com/one/two/>
;<

*:

	asTag = |<https://example.com/Foo#bar1>|*:
	  test = "first"
	;

	asBareTag = |<https://example.com/Foo#bar2>|

	asTagWithTypeTag = |<https://example.com/Foo#bar3>|*|<https://example.com/Foo>|:
	  test = "third"
	;

	asLabelAndTag = |"bar4"|*|<https://example.com/Foo>|:
	  test = "fourth"
	;

	asLabelAndHandle = |"bar5"|*Foo:
	  test = "fifth"
	;

	asLabelAndNamespaceHandle = |"bar6"|*count/Foo:
	  test = "sixth"
	;

	asTagAndNamespaceHandle = |<https://example.com/one/two/Foo#bar7>|*count/Foo:
	  test = "seventh"
	;

	asHandle = Foo#bar8:
	  test = "eighth"
	;

	asNamespaceHandle = count/Foo#bar9:
	  test = "ninth"
	;

;
